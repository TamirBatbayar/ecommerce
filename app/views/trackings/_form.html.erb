<%= simple_form_for(@tracking) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :session_id %>
    <%= f.input :product_id %>
    <%= f.input :page_name %>
    <%= f.input :detail %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
